{
    "custom/power": {
        "format": "⏻",
        "on-click": "shutdown now",
        "on-click-right": "reboot"
    },
    "hyprland/workspaces": {
        "format": "{icon} [ {windows} ]",
        "on-click": "activate",
        "format-icons": {
            "1": "一",
            "2": "二",
            "3": "三",
            "4": "四",
            "5": "五",
            "6": "六",
            "7": "七",
            "8": "八",
            "9": "九",
            "10": "十",
            "special": "零",
            "urgent": "止"
            //"active": "󰣇"
        },
        "persistent-workspaces": {
            "DP-2": [1],
            "HDMI-A-1": [2]
        },
        "show-special": true,
        "special-visible-only": true,
        "format-window-separator": ", ",
        "window-rewrite-default": "",
        "window-rewrite": {
            "class<firefox>": "",
            "class<firefox> title<.*youtube.*>": "",
            "class<firefox> title<.*github.*>": "",
            "class<firefox> title<.*chatgpt.*>": "",
            "class<zen.*>": "󰰷",
            "class<zen.*> title<.*youtube.*>": "",
            "class<zen.*> title<.*github.*>": "",
            "class<zen.*> title<.*chatgpt.*>": "",
            "class<libreoffice-writer": "",
            "class<obsidian>": "ᛞ",
            "class<neovide>": "",
            "class<kitty>": "󰄛",
            "thunderbird": "",
            "ticktick": "",
            "wezterm": "",
            "spotify": "󰓇",
            "chrome": ""
        }
    },
    "tray": {
        "icon-size": 16,
        "spacing": 10
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-muted": "󰝟",
        "format-icons": {
            "headphone": "",
            "phone": "",
            "portable": "",
            "car": "",
            "default": ["", "", ""]
        },
        "on-click": "pavucontrol"
    },
    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format": "{:%a, %b %d, %I:%M %p}"
    },
    "custom/center": {
        "format": ""
    },
    "hyprland/window": {
        "format": " {initialTitle}"

    },
    "custom/l_end": {
        "format": " ",
        "interval": "once",
        "tooltip": false
    },
    "custom/r_end": {
        "format": " ",
        "interval": "once",
        "tooltip": false
    },
    "custom/padd": {
        "format": "  ",
        "interval": "once",
        "tooltip": false
    }
}
